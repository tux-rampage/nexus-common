{
    "name" : "tuxrampage/nexus-common",
    "description" : "Common classes for Nexus PHP Deployment",
    "repositories" : [{
            "type" : "git",
            "url" : "git@github.com:tux-rampage/zend-di.git"
        }
    ],
    "require" : {
        "php": "^7.1",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb-odm": "^1.2",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0.0",
        "symfony/console": "^3.1",
        "zendframework/zend-config": "^2.5 | ^3.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-crypt": "^3.0",
        "zendframework/zend-di": "^3.0",
        "zendframework/zend-expressive": "^3.0",
        "zendframework/zend-expressive-helpers": "*",
        "zendframework/zend-filter": "^2.7 | ^3.0",
        "zendframework/zend-json": "^2.5 | ^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-validator": "^2.8 | ^3.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^6.0",
        "squizlabs/php_codesniffer" : "^2.3",
        "filp/whoops" : "^1.1 | ^2.0"
    },
    "license" : "GPL-3.0+",
    "keywords" : [
        "rampage",
        "deployment"
    ],
    "autoload" : {
        "psr-4" : {
            "Rampage\\Nexus\\" : "src/"
        }
    },
    "type" : "library",
    "config" : {
        "discard-changes" : true,
        "sort-packages": true
    },
    "authors" : [{
            "name" : "Axel Helmert",
            "email" : "staff@pro-unreal.de",
            "role" : "lead"
        }
    ],
    "homepage" : "https://github.com/tux-rampage/nexus-common",
    "prefer-stable" : true,
    "minimum-stability" : "dev",
    "extra" : {
        "banch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
