{
    "name" : "rampage-php/nexus-common",
    "description" : "Common classes for Rampage-Nexus PHP Deployment",
    "repositories" : [{
            "type" : "git",
            "url" : "git@github.com:tux-rampage/zend-di.git"
        }
    ],
    "require" : {
        "php" : "^5.6 || ^7.0",
        "zendframework/zend-expressive" : "^1.0",
        "zendframework/zend-expressive-helpers" : "^2.0",
        "zendframework/zend-stdlib" : "^3.0",
        "zendframework/zend-validator" : "^2.8 | ^3.0",
        "zendframework/zend-crypt" : "^3.0",
        "zendframework/zend-expressive-fastroute" : "^1.0",
        "zendframework/zend-servicemanager" : "^3.0",
        "zendframework/zend-di" : "^3.0",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-json": "^2.5 | ^3.0",
        "zendframework/zend-filter" : "^2.7 | ^3.0",
        "zendframework/zend-config" : "^2.5 | ^3.0",
        "doctrine/collections": "^1.3",
        "guzzlehttp/guzzle" : "^6.0",
        "mtymek/expressive-config-manager" : "^0.4.0",
        "symfony/console" : "^3.1",
        "psr/log" : "^1.0.0"
    },
    "require-dev" : {
        "zendframework/zend-di" : "dev-zf3 as 3.0.0",

        "phpunit/phpunit" : "^4.8",
        "squizlabs/php_codesniffer" : "^2.3",
        "filp/whoops" : "^1.1 || ^2.0"
    },
    "license" : "GPL-3.0+",
    "keywords" : [
        "rampage",
        "deployment"
    ],
    "autoload" : {
        "psr-4" : {
            "Rampage\\Nexus\\" : "src/"
        }
    },
    "type" : "library",
    "config" : {
        "discard-changes" : true
    },
    "authors" : [{
            "name" : "Axel Helmert",
            "email" : "staff@pro-unreal.de",
            "role" : "lead"
        }
    ],
    "homepage" : "https://github.com/tux-rampage/rampage-nexus",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "banch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
